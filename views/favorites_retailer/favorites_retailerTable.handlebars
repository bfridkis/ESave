<div>
	<h1>{{title}}</h1>
	<table>
		<thead>
			<th>User</th>
			<th>Retailer</th>
		</thead>
		<tbody>
			{{#each favorites_retailer}}
			<tr>
				<td>{{user}}</td>
				<td>{{retailer}}</td>
				<td class="deleteButton"><button onclick="deleteRow({{id}}, 'favorites_retailer')">Delete</button></td>
			</tr>
			{{/each}}
		</tbody>
	</table>
	<form action="/dev1" method="get">
		<button class="homePage" type="submit" value="Home Page" name="Home Page">Home Page</button>
	</form>
	{{#each jsscriptsTableView}}
		<script src="{{this}}"></script>
	{{/each}}
</div>
