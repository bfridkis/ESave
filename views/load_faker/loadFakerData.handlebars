<div style="margin:auto;height:300px;position:absolute;top:0;bottom:0;left:0;right:0;text-align:center">
	<h1>Load Sample Data</h1>
	<h2>Please Enter Quantities Indicated and Press Submit</h2>
	<label for="products">Products</label>
	<input type="number" name="numProds" id="products" value="0" min="0"
		style="margin-top:50px;max-width:60px;text-align:center"></input>
	<label for="products">Retailers</label>
	<input type="number" name="numRets" id="Retailers" value="0" min="0"
		style="max-width:60px;text-align:center"></input>
	<label for="products">Retailer_Products</label>
	<input type="number" name="numRetProds" id="Retailer_Products" value="0" min="0"
		style="max-width:60px;text-align:center"></input>
	<label for="products">Promotions</label>
	<input type="number" name="numPromos" id="Promotions" value="0" min="0"
		style="max-width:60px;text-align:center"></input><br>
	<label for="products">Password</label>
	<input type="text" name="password" id="Password" style="margin-top:30px"></input><br>

	<button id="submit" style="margin-top:10px">Submit</button>
	<form action="/dev1" method="get">
		<button class="homePage" type="submit" value="Home Page" name="Home Page">Home Page</button>
	</form>
	{{#each jsscriptsFaker}}
		<script src="{{this}}"></script>
	{{/each}}
</div>

<script defer>loadFakerData();</script>
