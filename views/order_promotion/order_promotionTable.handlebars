<div>
	<h1>{{title}}</h1>
	<table>
		<thead>
			<th>Order</th>
			<th>Promotion</th>
		</thead>
		<tbody>
			{{#each order_promotion}}
			<tr>
				<td>{{order}}</td>
				<td>{{promotion}}</td>
				<td class="deleteButton"><button onclick="deleteRow({{id}}, 'order_promotion')">Delete</button></td>
			</tr>
			{{/each}}
		</tbody>
	</table>
	<form action="/dev1" method="get">
		<button class="homePage" type="submit" value="Home Page" name="Home Page">Home Page</button>
	</form>
	{{#each jsscriptsTableView}}
		<script src="{{this}}"></script>
	{{/each}}
</div>
